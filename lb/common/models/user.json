    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "info": {
      "type": "string"
    },
    "type": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
